{
  "name": "purlieu",
  "version": "1.0.0",
  "private": true,
  "description": "Distance projection project",
  "main": "./src/index.js",
  "engines": {
    "node": "10.15.3"
  },
  "repository": "github:laujonat/purlieu",
  "engineStrict": true,
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "@babel/runtime": "^7.2.0",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-polyfill": "^6.26.0",
    "dotenv": "^7.0.0",
    "dotenv-webpack": "^1.5.5",
    "lyft-node": "0.0.5",
    "nodemon": "^1.18.7",
    "prop-types": "^15.6.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-fontawesome": "^1.6.1",
    "react-router-dom": "^5.0.0",
    "react-test-renderer": "^16.7.0",
    "styled-components": "^4.1.2"
  },
  "scripts": {
    "build": "webpack --mode production",
    "lint": "eslint 'client/**/*.{js,jsx}'",
    "lint-fix": "npm run lint --fix && npm run prettier-fix",
    "prettier": "prettier --list-different --no-semi './**/*.{js,jsx}' './**/*.css **/*.json'",
    "prettier-fix": "prettier --write --no-semi './**/*.{js,jsx}' './**/*.css **/*.json'",
    "test": "npm run update:snapshot && npm run lint",
    "update:snapshot": "jest --updateSnapshot",
    "test:watch": "jest --watch --verbose",
    "watch": "webpack-dev-server --mode development --watch",
    "dev": "nodemon --exec babel-node ./server/index.js",
    "start": "node ./server/index.js",
    "postinstall": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@google/maps": "^0.5.5",
    "babel": "^6.23.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "cors": "^2.8.5",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^24.7.1",
    "prettier": "^1.15.3",
    "react-redux": "^7.0.2",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.2",
    "redux-saga-test-plan": "^3.7.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "jest": {
    "verbose": true,
    "clearMocks": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "client/**.{js, jsx}",
      "!client/*.{js,jsx}",
      "!client/lib/**.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "client/*.{js, jsx}"
    ],
    "moduleDirectories": [
      "node_modules",
      "client"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node"
    ],
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/client/testSetup.js"
    ],
    "testMatch": [
      "**/?(*.)+(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "\\\\node_modules\\\\"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.jsx$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "browserslist": "> 0.25%, not dead",
  "author": "Jonathan Lau, Andre Le",
  "license": "MIT"
}

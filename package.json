{
  "name": "purlieu",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-polyfill": "^6.26.0",
    "dotenv": "^5.0.1",
    "dotenv-webpack": "^1.5.5",
    "lyft-node": "0.0.5",
    "nodemon": "^1.18.7",
    "prop-types": "^15.6.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-modal": "^3.4.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^2.1.1",
    "react-test-renderer": "^16.7.0",
    "styled-components": "^4.1.2"
  },
  "scripts": {
    "lint": "eslint --ignore-pattern !.eslintrc.js .eslintrc.js",
    "lint-fix": "yarn lint --fix && yarn prettier-fix",
    "prettier": "prettier --list-different --no-semi './**/*.{js,jsx}' './**/*.css **/*.json'",
    "prettier-fix": "prettier --write --no-semi './**/*.{js,jsx}' './**/*.css **/*.json'",
    "test": "jest --coverage",
    "update:snapshot": "jest --updateSnapshot",
    "test:watch": "jest --watch --verbose",
    "watch": "webpack --watch",
    "dev": "webpack && nodemon --exec babel-node -- ./bin/www",
    "start": "webpack && node ./bin/www "
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@google/maps": "^0.5.5",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "8",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "cors": "^2.8.5",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.2",
    "redux-saga-test-plan": "^3.7.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.1.2"
  },
  "jest": {
    "verbose": true,
    "clearMocks": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "client/**.{js, jsx}",
      "!client/*.{js,jsx}",
      "!client/lib/**.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "client/*.{js, jsx}"
    ],
    "moduleDirectories": [
      "node_modules",
      "client"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node"
    ],
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "<rootDir>/client/testSetup.js",
    "testMatch": [
      "**/?(*.)+(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "\\\\node_modules\\\\"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.jsx$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "description": "Distance projection project",
  "main": "./client/index.js",
  "repository": "https://github.com/laujonat/purlieu.io.git",
  "author": "Jonathan Lau",
  "license": "MIT"
}
